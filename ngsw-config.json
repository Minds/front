{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "https://**/api/v**",
        "https://**/plugins/**",
        "http://localhost:8080/api/**",
        "https://**/emails/unsubscribe/**",
        "http://localhost:8080/emails/unsubscribe/**"
      ],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "1h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**",
    "!/embed/**",
    "!/storybook",
    "!/storybook/**",
    "!/fs/**",
    "!/icon/**"
  ],
  "appData": {
    "commit": "COMMITREF",
    "pipeline": "VERSION"
  }
}
