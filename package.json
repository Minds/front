{
  "name": "minds",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "preinstall": "git config core.hooksPath .git/hooks/",
    "build": "ng build --prod",
    "prebuild:dev": "gulp build.sass --deploy-url=/",
    "build:dev": "ng build --watch=true --poll=1000",
    "preserve:dev": "gulp build.sass --deploy-url=http://localhost:4200/",
    "serve:dev": "ng serve --deploy-url=http://localhost:4200/ --watch=true --poll=1000 --progress --proxy-config proxy.conf.js --host=0.0.0.0 --disableHostCheck=true",
    "test": "ng test --source-map=false",
    "lint": "ng lint",
    "e2e": "cypress run --debug",
    "e2e-open": "cypress open",
    "postinstall": "node patch.js && ngcc",
    "serve:ssr": "node dist/server/main",
    "build:ssr": "ng run minds:server:production",
    "build:ssr:dev": "ng run minds:server:production",
    "build:embed": "ng run minds-embed:build",
    "serve:embed:dev": "ng serve minds-embed --deploy-url=http://localhost:4300/ --watch=true --poll=1000 --progress --proxy-config proxy.conf.js --host=0.0.0.0 --port=4300 --disableHostCheck=true",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json",
    "xi18n": "gulp extract.i18n",
    "xi18n-import": "gulp import.i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/localize": "^9.1.12",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/platform-server": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@bhayward93/ckeditor5-build-minds": "^1.1.7",
    "@ckeditor/ckeditor5-angular": "^1.2.3",
    "@danielmoncada/angular-datetime-picker": "^9.3.1",
    "@mindsorg/web3modal-angular": "^1.2.14",
    "@gorniv/ngx-universal": "^2.2.2",
    "@nguniversal/builders": "~9.1.1",
    "@nguniversal/common": "~9.1.1",
    "@nguniversal/express-engine": "~9.1.1",
    "@nguniversal/module-map-ngfactory-loader": "~8.2.6",
    "@nguniversal/socket-engine": "~9.1.1",
    "@transak/transak-sdk": "^1.0.25",
    "@sentry/browser": "^5.27.3",
    "@walletconnect/web3-provider": "^1.2.2",
    "angular-plotly.js": "^2.0.0",
    "bn.js": "^4.11.8",
    "braintree-web": "3.41.0",
    "cookie-parser": "^1.4.4",
    "core-js": "~2.6.2",
    "ethers": "^5.0.18",
    "ethjs": "~0.4.0",
    "ethjs-account": "^0.1.4",
    "ethjs-provider-signer": "^0.1.4",
    "ethjs-signer": "0.1.1",
    "express-http-proxy": "^1.6.0",
    "fortmatic": "^2.2.1",
    "global": "^4.3.2",
    "highlight.js": "^10.1.1",
    "macy": "^2.5.1",
    "marked": "^1.1.1",
    "material-design-icons": "~3.0.1",
    "material-design-lite": "~1.3.0",
    "medium-editor": "^5.23.2",
    "moment": "^2.24.0",
    "ngx-drag-drop": "^2.0.0",
    "ngx-image-cropper": "^3.1.6",
