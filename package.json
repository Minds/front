{
  "name": "minds",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "preinstall": "git config core.hooksPath .git/hooks/",
    "build": "ng build --configuration production",
    "prebuild:dev": "gulp build.sass",
    "build:dev": "ng build --watch=true --poll=1000",
    "preserve:dev": "gulp build.sass",
    "serve:dev": "ng serve --configuration hmr --hmr --proxy-config proxy.conf.js --host=0.0.0.0 minds --allowed-hosts=all",
    "test": "ng test --source-map=false",
    "test:dev": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng test --source-map=false --browsers=ChromeHeadlessCI",
    "lint": "prettier --check 'src/**/*.{ts,scss,html}'",
    "lint:dev": "prettier --write 'src/**/*.{ts,scss,html}'",
    "postinstall": "ngcc",
    "serve:ssr": "node dist/server/main",
    "build:ssr": "ng run minds:server:production",
    "build:ssr:dev": "ng run minds:server:production",
    "build:embed": "ng run minds-embed:build:production",
    "serve:embed:dev": "ng serve minds-embed --watch=true --poll=1000 --proxy-config proxy.conf.js --host=0.0.0.0 --port=4300 --disableHostCheck=true",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json",
    "xi18n": "gulp extract.i18n",
    "xi18n-import": "gulp import.i18n",
    "prepare": "husky install",
    "docs:json": "compodoc -p ./tsconfig.doc.json -e json -d .",
    "storybook": "npm run docs:json && ng run minds:storybook",
    "build-storybook": "npm run docs:json && ng run minds:build-storybook"
  },
