  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.11",
    "@angular/cli": "^9.1.11",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@angularclass/hmr": "^2.1.3",
    "@locl/cli": "0.0.1-beta.9",
    "@types/express-serve-static-core": "^4.17.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.4",
    "@types/node": "~14.0.1",
    "cross-env": "^7.0.2",
