    "gulp": "~4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-minify-css": "^1.1.6",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-template": "^5.0.0",
    "husky": "^3.0.4",
    "jasmine-core": "~2.99.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine-ts-async": "^1.0.0",
    "jsonwebtoken": "8.5.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.2",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "xmlbuilder2": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh .githooks/pre-commit && pretty-quick --staged --bail --pattern \"**/*.*(ts|html|scss)\"",
      "post-commit": "sh .githooks/post-commit"
    }
  }
}
